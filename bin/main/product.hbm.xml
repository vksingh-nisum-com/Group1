<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.nisum.dto.Product" table="catalog">
		<id name="Id" type="int" column="Id">
			<generator class="assigned" />
		</id>
		
		<property name="PId"><column name="Pid" /></property>
		<property name="PName"><column name="Pname" /></property>
		<property name="Price"><column name="Price" /></property>
		<property name="Descrip"><column name="Descrip" /></property>
		<property name="Img"><column name="Img" /></property>
		
	
	</class>
</hibernate-mapping>